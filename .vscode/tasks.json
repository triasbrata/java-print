{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build-class",
      "type": "shell",
      "command": [
        "javac -cp lib/gson-2.6.2.jar:lib/pdfbox-app-2.0.16.jar  src/app/App.java -d out/  --release 8"
      ],
      "group": "build"
    },
    {
      "label": "build-jar",
      "dependsOn": [
        "build-class"
      ],
      "type": "shell",
      "command":"jar",
      "args": [
        "-C out",
        "--file ${input:outputDir}/PrintDirect.jar",
        "--manifest manifest.MF", 

      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": []
    },
  ],
  "inputs": [
    {
      "id": "outputDir",
      "description": "Out put directory",
      "default": "/Users/triasbratayudhana/Developer/Node/wexond/static",
      "type": "promptString"
    }
  ]
}